        @-webkit-keyframes animate-preloader_admin {
            0% {
                transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
            }
        }
        
        @keyframes animate-preloader_admin {
            0% {
                transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
