            <script>
                $(window).on("load", function () {

                    var success = '';
                    var danger = '';
                    var error = '';
                    var warning = '';

                    if (success) {
                        $("#toastBasic4").toast('show');
                    }

                    if (danger || error) {
                        $("#toastBasic3").toast('show');
                    }

                    if (warning) {
                        $("#toastBasic2").toast('show');
                    }
                });
            </script>

            <div class="card">
