        #preloader_admin {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            z-index: 9999;
            overflow: hidden;
            background: #FAFAFA;
            display: block;
        }
        
        main {
            position: relative;
        }
        
        #preloader_admin:before {
            content: "";
            position: fixed;
            top: calc(50% - 30px);
            left: calc(50% - 30px);
            border: 6px solid #106eea;
            border-top-color: #e2eefd;
            border-radius: 50%;
            width: 60px;
            height: 60px;
            -webkit-animation: animate-preloader_admin 1s linear infinite;
            animation: animate-preloader_admin 1s linear infinite;
